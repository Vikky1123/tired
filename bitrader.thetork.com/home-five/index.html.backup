<script>
    jQuery(document).ready(function($) {
        AOS.init();

        // Investment calculator functionality
        $('#calculateBtn').on('click', function() {
            const investmentAmount = parseFloat($('#investmentAmount').val()) || 0;
            const selectedPlan = $('#investmentPlan').val();
            let returnRate = 0;
            
            // Set return rate based on selected plan
            switch(selectedPlan) {
                case 'starter':
                    returnRate = Math.random() * (0.35 - 0.30) + 0.30; // 30-35%
                    break;
                case 'silver':
                    returnRate = Math.random() * (0.37 - 0.32) + 0.32; // 32-37%
                    break;
                case 'gold':
                    returnRate = Math.random() * (0.40 - 0.35) + 0.35; // 35-40%
                    break;
                case 'platinum':
                    returnRate = Math.random() * (0.42 - 0.37) + 0.37; // 37-42%
                    break;
                case 'diamond':
                    returnRate = Math.random() * (0.44 - 0.40) + 0.40; // 40-44%
                    break;
                case 'elite':
                    returnRate = Math.random() * (0.45 - 0.42) + 0.42; // 42-45%
                    break;
                default:
                    returnRate = 0.30;
            }
            
            const expectedReturn = investmentAmount * returnRate;
            const totalReturn = investmentAmount + expectedReturn;
            
            // Update result
            $('#returnRateDisplay').text((returnRate * 100).toFixed(2) + '%');
            $('#expectedReturn').text('$' + expectedReturn.toFixed(2));
            $('#totalReturn').text('$' + totalReturn.toFixed(2));
            
            // Show result div
            $('#calculatorResult').removeClass('d-none').addClass('d-block');
        });
    });
</script>


