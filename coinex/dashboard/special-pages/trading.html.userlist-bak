    <!-- Real-time Order Book Updates -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Get table references
            const marketDepthTable = document.querySelector('#pills-description .iq-order-book tbody');
            const orderVolumeTable = document.querySelector('#pills-additional-information .iq-order-book tbody');
            
            // Base values for price and volume
            const baseBuyPrice = 34286.28;
            const baseSellPrice = 34350.90;
            const baseVolume = 0.58399;
            const baseMarketCap = 0.05598;
            const base1DHigh = 183.8801;
            const base1DLow = 164.2008;
            
            // Volatility factors
            const priceFactor = 30; // Price volatility
            const volumeFactor = 0.05; // Volume volatility
            const marketCapFactor = 0.01; // Market cap volatility
            const highLowFactor = 2; // 1D high/low volatility
            
            // CSS for animations
            const style = document.createElement('style');
            style.textContent = `
                @keyframes fade-highlight {
                    0% { background-color: rgba(255, 151, 29, 0.3); }
                    100% { background-color: transparent; }
                }
                
                .highlight-row {
                    animation: fade-highlight 1.5s ease-out forwards;
                }
                
                @keyframes flash-green {
                    0% { color: #ffffff; }
                    50% { color: #00c853; }
                    100% { color: #00c853; }
                }
                
                @keyframes flash-red {
                    0% { color: #ffffff; }
                    50% { color: #f44336; }
                    100% { color: #f44336; }
                }
                
                .flash-green {
                    animation: flash-green 1s ease-out forwards;
                }
                
                .flash-red {
                    animation: flash-red 1s ease-out forwards;
                }
            `;
            document.head.appendChild(style);
            
            // Format numbers with commas
            function formatNumber(num) {
                return num.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
            }
            
            // Generate random data for a row
            function generateRowData() {
                // Random fluctuations around base values
                const direction = Math.random() > 0.5 ? 1 : -1;
                const priceChange = direction * Math.random() * priceFactor;
                
                const buyPrice = baseBuyPrice + priceChange * 0.98; // Buy price slightly lower
                const sellPrice = baseSellPrice + priceChange * 1.02; // Sell price slightly higher
                
                // Volume fluctuates both ways
                const volumeChange = (Math.random() - 0.5) * volumeFactor;
                const volume = baseVolume + volumeChange;
                
                // Market cap follows price somewhat
                const marketCapChange = direction * Math.random() * marketCapFactor;
                const marketCap = baseMarketCap + marketCapChange;
                
                // High and low values
                const highChange = Math.abs(Math.random() * highLowFactor);
                const lowChange = -Math.abs(Math.random() * highLowFactor);
                const dayHigh = base1DHigh + highChange;
                const dayLow = base1DLow + lowChange;
                
                return {
                    volume: volume.toFixed(5),
                    buyPrice: formatNumber(buyPrice.toFixed(2)),
                    sellPrice: formatNumber(sellPrice.toFixed(2)),
                    marketCap: marketCap.toFixed(5),
                    dayHigh: dayHigh.toFixed(4),
                    dayLow: dayLow.toFixed(4),
                    endVolume: marketCap.toFixed(5)
                };
            }
            
            // Create a row with the generated data
            function createRow(data, previousData = null) {
                const row = document.createElement('tr');
                row.classList.add('highlight-row');
                
                // Direction indicators for price changes
                let volumeClass = previousData ? (parseFloat(data.volume) > parseFloat(previousData.volume) ? 'flash-green' : 'flash-red') : 'text-success';
                let buyPriceClass = previousData ? (parseFloat(data.buyPrice.replace(/,/g, '')) > parseFloat(previousData.buyPrice.replace(/,/g, '')) ? 'flash-green' : 'flash-red') : '';
                let sellPriceClass = previousData ? (parseFloat(data.sellPrice.replace(/,/g, '')) > parseFloat(previousData.sellPrice.replace(/,/g, '')) ? 'flash-green' : 'flash-red') : 'text-danger';
                
                row.innerHTML = `
                    <td class="${volumeClass}">${data.volume}</td>
                    <td class="${buyPriceClass}">${data.buyPrice}</td>
                    <td class="${sellPriceClass}">${data.sellPrice}</td>
                    <td>${data.marketCap}</td>
                    <td>${data.dayHigh}</td>
                    <td>${data.dayLow}</td>
                    <td>${data.endVolume}</td>
                `;
                
                return row;
            }
            
            // Store previous data for comparison
            let previousMarketDepthData = [];
            let previousOrderVolumeData = [];
            
            // Update a specific table
            function updateTable(tableElement, previousDataArray, isMarketDepth = true) {
                // Clear existing animations
                const existingRows = tableElement.querySelectorAll('tr');
                existingRows.forEach(row => {
                    row.classList.remove('highlight-row');
                    const cells = row.querySelectorAll('td');
                    cells.forEach(cell => {
                        cell.classList.remove('flash-green', 'flash-red');
                    });
                });
                
                // Generate new data for each row
                const newData = [];
                for (let i = 0; i < 10; i++) {
                    const rowData = generateRowData();
                    newData.push(rowData);
                    
                    // Create or update row
                    if (existingRows[i]) {
                        // Update existing row
                        const previousRow = previousDataArray[i] || null;
                        const newRow = createRow(rowData, previousRow);
                        tableElement.replaceChild(newRow, existingRows[i]);
                    } else {
                        // Create new row
                        const newRow = createRow(rowData);
                        tableElement.appendChild(newRow);
                    }
                }
                
                // Store the new data for the next update
                if (isMarketDepth) {
                    previousMarketDepthData = newData;
                } else {
                    previousOrderVolumeData = newData;
                }
            }
            
            // Function to update both tables with random timing
            function updateTables() {
                // Update Market Depth table
                updateTable(marketDepthTable, previousMarketDepthData, true);
                
                // Update Order Volume table
                updateTable(orderVolumeTable, previousOrderVolumeData, false);
                
                // Schedule next update with random interval (0.5-3 seconds)
                const nextUpdateDelay = Math.random() * 2500 + 500;
                setTimeout(updateTables, nextUpdateDelay);
            }
            
            // Start the updates after a short delay
            setTimeout(() => {
                // Clear the existing static data
                marketDepthTable.innerHTML = '';
                orderVolumeTable.innerHTML = '';
                
                // Start the updates
                updateTables();
                
                // Log to console that the real-time updates have started
                console.log('Real-time Order Book updates started');
            }, 1000);
        });
    </script>
    
    <!-- Real-time Trade History Updates -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Get reference to the trade history table body
            const tradeHistoryBody = document.getElementById('trade-history-body');
            
            // Base price values (same as in Order Book for consistency)
            const baseBuyPrice = 34286.28;
            const baseSellPrice = 34350.90;
            const baseVolume = 0.05546;
            
            // Maximum number of rows to display in the table
            const maxRows = 20;
            
            // Configuration for trade generation behavior
            const config = {
                priceVolatility: 50, // How much prices can fluctuate
                volumeVolatility: 0.01, // How much volume can fluctuate
                upProbability: 0.4, // Probability of an upward trade (vs downward)
                initialTrades: 10, // Number of trades to generate at start
                fastUpdateProbability: 0.3, // Probability of a quick succession of trades
                fastUpdateCount: { min: 2, max: 5 }, // Range for number of quick succession trades
                fastUpdateSpeed: { min: 100, max: 300 }, // Speed of quick succession trades (ms)
                normalUpdateSpeed: { min: 1000, max: 4000 } // Normal trade update interval (ms)
            };
            
            // SVG templates for up/down arrows
            const upArrowSVG = `<svg width="20" viewBox="0 0 20 21" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M5.77132 1.0415L5.77132 13.5415" stroke="#00EC42" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M0.751157 6.08301L5.77116 1.04134L10.792 6.08301" stroke="#00EC42" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>`;
            
            const downArrowSVG = `<svg width="20" viewBox="0 0 20 21" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M6.22868 13.9585V1.4585" stroke="#FF2E2E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path d="M11.2488 8.91699L6.22884 13.9587L1.20801 8.91699" stroke="#FF2E2E" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>`;
            
            // CSS for new trade animation
            const style = document.createElement('style');
            style.textContent = `
                @keyframes new-trade-animation {
                    0% { background-color: rgba(255, 151, 29, 0.3); }
                    100% { background-color: transparent; }
                }
                
                .new-trade {
                    animation: new-trade-animation 2s ease-out forwards;
                }
                
                #trade-history-body {
                    max-height: 600px;
                    overflow-y: auto;
                }
                
                #trade-history-body tr:first-child {
                    border-left: 2px solid #FF971D;
                }
            `;
            document.head.appendChild(style);
            
            // Format number with commas
            function formatNumber(num) {
                return num.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
            }
            
            // Format time as HH:MM:SS
            function formatTime() {
                const now = new Date();
                const hours = now.getHours().toString().padStart(2, '0');
                const minutes = now.getMinutes().toString().padStart(2, '0');
                const seconds = now.getSeconds().toString().padStart(2, '0');
                return `${hours}:${minutes}:${seconds}`;
            }
            
            // Generate a random trade
            function generateTrade(lastTrade = null) {
                // Determine if it's an up or down trade
                const isUp = Math.random() < config.upProbability;
                
                // Add some randomness to the price
                const priceFactor = (Math.random() * config.priceVolatility);
                const volumeFactor = (Math.random() * config.volumeVolatility);
                
                // If there's a last trade, use it as the baseline to create a price pattern
                let buyPrice, sellPrice, volume;
                
                if (lastTrade) {
                    // Extract numbers from strings with commas
                    const lastBuyPrice = parseFloat(lastTrade.buyPrice.replace(/,/g, ''));
                    const lastSellPrice = parseFloat(lastTrade.sellPrice.replace(/,/g, ''));
                    const lastVolume = parseFloat(lastTrade.volume);
                    
                    // Create a continuing pattern
                    const direction = isUp ? 1 : -1;
                    buyPrice = lastBuyPrice + (direction * priceFactor);
                    sellPrice = lastSellPrice + (direction * priceFactor * 1.02); // Sell price slightly higher
                    
                    // Volume is more random but related to direction
                    const volumeDirection = Math.random() > 0.3 ? direction : -direction; // Sometimes opposite
                    volume = Math.max(0.00001, lastVolume + (volumeDirection * volumeFactor));
                } else {
                    // Start with base values
                    const direction = isUp ? 1 : -1;
                    buyPrice = baseBuyPrice + (direction * priceFactor);
                    sellPrice = baseSellPrice + (direction * priceFactor);
                    volume = baseVolume + ((Math.random() - 0.5) * volumeFactor * 2);
                }
                
                // Format values
                return {
                    buyPrice: formatNumber(buyPrice.toFixed(2)),
                    sellPrice: formatNumber(sellPrice.toFixed(2)),
                    volume: volume.toFixed(5),
                    time: formatTime(),
                    isUp: isUp
                };
            }
            
            // Create a table row for a trade
            function createTradeRow(trade) {
                const row = document.createElement('tr');
                row.className = trade.isUp ? 'growing-stock new-trade' : 'falling-stock new-trade';
                
                row.innerHTML = `
                    <td>
                        ${trade.isUp ? upArrowSVG : downArrowSVG}${trade.buyPrice}
                    </td>
                    <td>${trade.volume}</td>
                    <td>${trade.sellPrice}</td>
                    <td>${trade.time}</td>
                `;
                
                return row;
            }
            
            // Add a new trade to the table
            function addTrade() {
                // Get the latest trade data if any exists
                const existingRows = tradeHistoryBody.querySelectorAll('tr');
                let lastTrade = null;
                
                if (existingRows.length > 0) {
                    const firstRow = existingRows[0];
                    const buyPriceCell = firstRow.querySelector('td:first-child');
                    const volumeCell = firstRow.querySelector('td:nth-child(2)');
                    const sellPriceCell = firstRow.querySelector('td:nth-child(3)');
                    const timeCell = firstRow.querySelector('td:nth-child(4)');
                    
                    lastTrade = {
                        buyPrice: buyPriceCell.textContent.trim().replace(/[^0-9,.]/g, ''),
                        volume: volumeCell.textContent.trim(),
                        sellPrice: sellPriceCell.textContent.trim(),
                        time: timeCell.textContent.trim(),
                        isUp: firstRow.classList.contains('growing-stock')
                    };
                }
                
                // Generate new trade based on the last one
                const newTrade = generateTrade(lastTrade);
                
                // Create and insert the new row at the top
                const newRow = createTradeRow(newTrade);
                tradeHistoryBody.insertBefore(newRow, tradeHistoryBody.firstChild);
                
                // Remove excess rows
                const rows = tradeHistoryBody.querySelectorAll('tr');
                if (rows.length > maxRows) {
                    for (let i = maxRows; i < rows.length; i++) {
                        tradeHistoryBody.removeChild(rows[i]);
                    }
                }
                
                return newTrade;
            }
            
            // Schedule the next trade update with variable timing
            function scheduleNextTradeUpdate(fastMode = false) {
                // Determine if we're going to have a burst of quick trades
                if (!fastMode && Math.random() < config.fastUpdateProbability) {
                    // Fast update mode - generate a series of quick trades
                    const burstCount = Math.floor(
                        Math.random() * (config.fastUpdateCount.max - config.fastUpdateCount.min + 1) + 
                        config.fastUpdateCount.min
                    );
                    
                    // Generate the burst of trades
                    let count = 0;
                    const generateBurst = () => {
                        addTrade();
                        count++;
                        
                        if (count < burstCount) {
                            const fastDelay = Math.floor(
                                Math.random() * (config.fastUpdateSpeed.max - config.fastUpdateSpeed.min + 1) + 
                                config.fastUpdateSpeed.min
                            );
                            setTimeout(generateBurst, fastDelay);
                        } else {
                            // After the burst, go back to normal timing
                            scheduleNextTradeUpdate();
                        }
                    };
                    
                    // Start the burst after a short delay
                    setTimeout(generateBurst, 500);
                } else {
                    // Normal timing between trades
                    const delay = Math.floor(
                        Math.random() * (config.normalUpdateSpeed.max - config.normalUpdateSpeed.min + 1) + 
                        config.normalUpdateSpeed.min
                    );
                    
                    setTimeout(() => {
                        addTrade();
                        scheduleNextTradeUpdate();
                    }, delay);
                }
            }
            
            // Initialize the table with some trades
            function initializeTradeHistory() {
                // Clear the existing static rows
                tradeHistoryBody.innerHTML = '';
                
                // Generate initial trades
                for (let i = 0; i < config.initialTrades; i++) {
                    const trade = generateTrade();
                    const row = createTradeRow(trade);
                    // Remove the animation class for initial trades
                    row.classList.remove('new-trade');
                    tradeHistoryBody.appendChild(row);
                }
                
                // Start the real-time updates
                scheduleNextTradeUpdate();
                console.log('Real-time Trade History updates started');
            }
            
            // Start the system after a short delay
            setTimeout(initializeTradeHistory, 1500);
        });
    </script>
    
    <!-- Real-time Market Overview Chart Updates -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Wait a bit for ApexCharts to initialize
            setTimeout(() => {
                // Check if the chart exists
                if (typeof ApexCharts === 'undefined' || !document.querySelector("#market-overview-chart")) {
                    console.error("ApexCharts not found or chart element doesn't exist.");
                    return;
                }
                
                // Get existing chart or create a new one
                let chart;
                try {
                    chart = ApexCharts.getChartByID("area-datetime");
                    if (!chart) {
                        console.error("Chart with ID 'area-datetime' not found, can't update.");
                        return;
                    }
                } catch (e) {
                    console.error("Error finding chart:", e);
                    return;
                }
                
                // Configuration for the live chart
                const config = {
                    initialDataPoints: 250, // How many data points to start with
                    maxDataPoints: 300, // Maximum number of data points to show
                    updateInterval: {
                        min: 1000, // Minimum time between updates (ms)
                        max: 3000  // Maximum time between updates (ms)
                    },
                    volatility: 0.8, // How volatile the price changes are
                    trendStrength: 0.7, // How strong trends are (vs random movements)
                    trendDuration: {
                        min: 10, // Minimum trend duration in data points
                        max: 30  // Maximum trend duration in data points
                    },
                    priceRange: {
                        min: 3000, // Minimum price
                        max: 4200  // Maximum price
                    },
                    highlightLatestPoint: true, // Whether to highlight the latest data point
                    cryptocurrency: {
                        ETH: { color: "#FF971D", volatility: 0.8, initialPrice: 3800 },
                        XMR: { color: "#00EC42", volatility: 1.2, initialPrice: 3500 },
                        LTC: { color: "#665FBF", volatility: 0.7, initialPrice: 3700 }
                    },
                    currentCrypto: "ETH" // Default cryptocurrency
                };
                
                // Subscribe to radio button changes for different cryptocurrencies
                const cryptoRadioButtons = document.querySelectorAll('input[name="flexRadioDefault"]');
                cryptoRadioButtons.forEach((radio, index) => {
                    radio.addEventListener('change', function() {
                        if (this.checked) {
                            const cryptos = Object.keys(config.cryptocurrency);
                            const newCrypto = cryptos[index % cryptos.length];
                            changeCryptocurrency(newCrypto);
                        }
                    });
                });
                
                // State variables
                let currentTrend = 1; // 1 for up, -1 for down
                let trendCounter = 0;
                let trendDuration = Math.floor(
                    Math.random() * (config.trendDuration.max - config.trendDuration.min + 1) + 
                    config.trendDuration.min
                );
                let latestTimestamp = new Date().getTime();
                let latestPrice = config.cryptocurrency[config.currentCrypto].initialPrice;
                let seriesData = [];
                
                // Generate initial data
                function generateInitialData() {
                    const endDate = new Date();
                    const startDate = new Date();
                    startDate.setDate(endDate.getDate() - 365); // 1 year of data
                    
                    seriesData = [];
                    let currentDate = new Date(startDate);
                    let price = config.cryptocurrency[config.currentCrypto].initialPrice;
                    
                    // Generate historical data points
                    while (currentDate <= endDate) {
                        if (trendCounter >= trendDuration) {
                            // Change trend direction
                            currentTrend = Math.random() > 0.5 ? 1 : -1;
                            trendDuration = Math.floor(
                                Math.random() * (config.trendDuration.max - config.trendDuration.min + 1) + 
                                config.trendDuration.min
                            );
                            trendCounter = 0;
                        }
                        
                        // Calculate price change with trend influence
                        const trendInfluence = Math.random() * config.trendStrength;
                        const randomness = Math.random() * (1 - config.trendStrength);
                        
                        // Trend direction affects the sign of the change
                        const direction = (trendInfluence > 0.5) ? currentTrend : (Math.random() > 0.5 ? 1 : -1);
                        
                        // Combine trend and randomness for price change
                        const volatility = config.cryptocurrency[config.currentCrypto].volatility;
                        const change = direction * (trendInfluence + randomness) * volatility * 30;
                        
                        // Apply price change
                        price += change;
                        
                        // Ensure price stays within desired range
                        price = Math.max(config.priceRange.min, Math.min(config.priceRange.max, price));
                        
                        // Add data point
                        seriesData.push([currentDate.getTime(), price]);
                        
                        // Move to next date (approximately daily data points)
                        currentDate.setDate(currentDate.getDate() + 1);
                        trendCounter++;
                    }
                    
                    // Store latest values
                    latestTimestamp = seriesData[seriesData.length - 1][0];
                    latestPrice = seriesData[seriesData.length - 1][1];
                    
                    return seriesData;
                }
                
                // Generate a new data point
                function generateNewDataPoint() {
                    if (trendCounter >= trendDuration) {
                        // Change trend direction
                        currentTrend = Math.random() > 0.5 ? 1 : -1;
                        trendDuration = Math.floor(
                            Math.random() * (config.trendDuration.max - config.trendDuration.min + 1) + 
                            config.trendDuration.min
                        );
                        trendCounter = 0;
                    }
                    
                    // Calculate price change with trend influence
                    const trendInfluence = Math.random() * config.trendStrength;
                    const randomness = Math.random() * (1 - config.trendStrength);
                    
                    // Trend direction affects the sign of the change
                    const direction = (trendInfluence > 0.5) ? currentTrend : (Math.random() > 0.5 ? 1 : -1);
                    
                    // Combine trend and randomness for price change
                    const volatility = config.cryptocurrency[config.currentCrypto].volatility;
                    const change = direction * (trendInfluence + randomness) * volatility * 10;
                    
                    // Apply price change
                    latestPrice += change;
                    
                    // Ensure price stays within desired range
                    latestPrice = Math.max(config.priceRange.min, Math.min(config.priceRange.max, latestPrice));
                    
                    // Update timestamp (a few minutes forward)
                    latestTimestamp += Math.floor(Math.random() * 300000) + 60000; // 1-6 minutes
                    
                    // Add new data point
                    seriesData.push([latestTimestamp, latestPrice]);
                    
                    // Remove old data points if we exceed the maximum
                    if (seriesData.length > config.maxDataPoints) {
                        seriesData.shift();
                    }
                    
                    trendCounter++;
                    return [latestTimestamp, latestPrice];
                }
                
                // Update chart with new data
                function updateChart() {
                    generateNewDataPoint();
                    
                    // Update the chart
                    chart.updateSeries([{
                        data: seriesData
                    }]);
                    
                    // Schedule next update
                    const delay = Math.floor(
                        Math.random() * (config.updateInterval.max - config.updateInterval.min + 1) + 
                        config.updateInterval.min
                    );
                    
                    setTimeout(updateChart, delay);
                }
                
                // Change the displayed cryptocurrency
                function changeCryptocurrency(crypto) {
                    if (config.cryptocurrency[crypto]) {
                        config.currentCrypto = crypto;
                        
                        // Update chart color
                        chart.updateOptions({
                            colors: [config.cryptocurrency[crypto].color]
                        });
                        
                        // Generate new data for this crypto
                        generateInitialData();
                        
                        // Update the chart
                        chart.updateSeries([{
                            data: seriesData
                        }]);
                        
                        console.log(`Switched to ${crypto} data`);
                    }
                }
                
                // Initialize with data
                generateInitialData();
                
                // Update series data
                chart.updateSeries([{
                    data: seriesData
                }]);
                
                // Update chart options - enable animations and highlight latest point
                chart.updateOptions({
                    chart: {
                        animations: {
                            enabled: true,
                            easing: 'easeinout',
                            speed: 800,
                            animateGradually: {
                                enabled: true,
                                delay: 150
                            },
                            dynamicAnimation: {
                                enabled: true,
                                speed: 350
                            }
                        }
                    },
                    colors: [config.cryptocurrency[config.currentCrypto].color],
                    markers: {
                        size: 0,
                        hover: {
                            size: 5
                        }
                    },
                    tooltip: {
                        x: {
                            format: 'dd MMM yyyy'
                        },
                        y: {
                            formatter: function(value) {
                                return '$' + value.toFixed(2);
                            }
                        }
                    }
                });
                
                // Start real-time updates
                setTimeout(updateChart, 2000);
                
                console.log('Real-time Market Overview Chart updates started');
            }, 2000); // Wait for ApexCharts to fully initialize
        });
    </script>
    
